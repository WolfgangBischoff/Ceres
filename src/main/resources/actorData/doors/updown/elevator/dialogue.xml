

<root name="elevator">

    <dialogue id="INIT-toFreightDeck" type="decision">
        <option nextDialogue="DECISION-toFreightDeck">[Control Deck]</option>
        <option>[Back]</option>
    </dialogue>

    <dialogue id="INIT-toCrewDeck" type="decision">
        <option nextDialogue="DECISION-toCrewDeck">[Crew Deck]</option>
        <option>[Back]</option>
    </dialogue>

    <dialogue id="NO_ENERGY" type="normal">
        <line>No energy</line>
    </dialogue>
    <dialogue id="NOISES" type="normal">
        <line>[You should check what's going on here first, what are those noises?]</line>
    </dialogue>
    <dialogue id="CHECK_LOCAL" type="normal">
        <line>[You should check what's going on here first.]</line>
    </dialogue>

    <dialogue id="DECISION-toCrewDeck" type="normal" levelchange="crewdeck/crewdeck" spawnID="elevator"/>

    <dialogue id="DECISION-toFreightDeck" type="normal" levelchange="transporter/transporter" spawnID="elevator"/>

    <dialogue id="INIT_TO_CERES" type="decision">
        <option nextDialogue="TO_CERES">[Use Space Elevator]</option>
        <option>[Back]</option>
    </dialogue>
    <dialogue id="TO_CERES" type="normal" levelchange="living_quarter/air_lock" spawnID="default"/>

    <dialogue id="INIT_A" type="normal">
        <line>You are on level A.</line>
    </dialogue>
    <dialogue id="INIT_toBiotech" type="decision">
        <option>[Back]</option>
    </dialogue>

</root>


